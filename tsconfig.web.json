{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src-qa/env.d.ts",
    "src/renderer/src-qa/**/*",
    "src/renderer/src-qa/**/*.vue",
    "src/renderer/src-network/env.d.ts",
    "src/renderer/src-network/**/*",
    "src/renderer/src-network/**/*.vue"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src-qa/*"
      ]
    }
  }
}
